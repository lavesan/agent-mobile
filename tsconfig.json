{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "strictPropertyInitialization": false,
    "baseUrl": "./src",
    "paths": {
      "@containers/*": ["containers/*"],
      "@components/*": ["components/*"],
      "@services/*": ["services/*"],
      "@models/*": ["models/*"],
      "@helpers/*": ["helpers/*"],
      "@config/*": ["config/*"],
      "@hooks/*": ["hooks/*"],
      "@styles/*": ["styles/*"]
    },
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  }
}
